        xmlns:local="clr-namespace:AuctionTrends"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <Label x:Name="label" Content="Function" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="functionSelect" HorizontalAlignment="Left" Margin="71,14,0,0" VerticalAlignment="Top" Width="166" SelectedIndex="0" SelectionChanged="comboBox_SelectionChanged">
            <ComboBoxItem Content="&lt;Please Select&gt;"/>
            <ComboBoxItem Content="Graph"/>
        </ComboBox>
        <Frame x:Name="functionFrame" Content="" HorizontalAlignment="Left" Height="638" Margin="10,41,0,0" VerticalAlignment="Top" Width="1252" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
