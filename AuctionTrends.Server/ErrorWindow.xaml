<Window x:Class="AuctionTrends.Server.ErrorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AuctionTrends.Server"
        mc:Ignorable="d"
        Title="ErrorWindow" Height="300" Width="300">
    <Grid>
        <Label x:Name="exceptionName" Content="Label" HorizontalAlignment="Left" Height="27" Margin="10,10,0,0" VerticalAlignment="Top" Width="272"/>
        <TextBlock x:Name="exceptionText" HorizontalAlignment="Left" Height="222" Margin="10,37,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="272"/>

    </Grid>
</Window>
